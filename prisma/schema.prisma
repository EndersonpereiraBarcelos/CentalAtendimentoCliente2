generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model tb_atendimento2 {
  id_atendimento     Int      @id @default(autoincrement())
  cpf                String   @db.VarChar(100)
  numero_atendimento String   @db.VarChar(15)
  inicio_atendimento DateTime @db.Timestamp(0)
  fim_atendimento    DateTime @db.DateTime(0)
}

model tb_atendimento3 {
  id_atendimento     Int      @id @default(autoincrement())
  cpf                String   @db.VarChar(100)
  numero_atendimento String   @db.VarChar(100)
  inicio_atendimento DateTime @default(now()) @db.Timestamp(0)
  fim_atendimento    DateTime @db.DateTime(0)
  sufixo             String   @db.VarChar(100)
